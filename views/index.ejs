<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/start.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <title>Super Cat Tales 2</title>
</head>

<body>
    <header>
        <!-- navigationBar -->

    </header>

    <section>
        <!-- intro -->
        <div class="startScreen">
            <img class="logo" src="/src/Sct2_logo.webp" alt="Super Cat Tales 2 Logo">
            <button class="playBtn rounded-pill btn-lg responsive-width">
                <span class="blink">tap to play</span>
            </button>
        </div>
    </section>
    <section>
        <!-- select screen -->
        <div class="selectScreen fadeOut">
            <div class="menyTitle">
                <p class="titleText">MENY</p>
            </div>
            <div class="menyScreen">
                <ul>
                     
                    <p class="textPlacement"><a href = "/howToPlay" class="menyText" role="button">
                        how to play</a></p>
                    <p class="textPlacement"><a href = "/catSelect" class="menyText" role="button">
                        Cats</a></p>
                    <p class="textPlacement"><a href = "/itemSelct" class="menyText" role="button">
                        items</a></p>
                    <p class="textPlacement"><a href = "/worldSelect" class="menyText" role="button">
                        worlds</a></p>
                </ul>

            </div>
        </div>
    </section>

    <footer></footer>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
    <script src="app.js"></script>
    <script src="/javascript/script.js"></script>

</body>

</html>